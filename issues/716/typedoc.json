{
  "entryPoints": ["src/index.ts"],
  "plugin": ["typedoc-plugin-markdown"],
  "readme": "none",
  "cleanOutputDir": true,
  "hidePageHeader": true,
  "hideGroupHeadings": true,
  "outputFileStrategy": "modules",
  "groupOrder": ["Function", "Interface", "*"],
  "disableSources": true,
  "outputs": [
    {
      "name": "html",
      "path": "./docs/html"
    },
    {
      "name": "markdown",
      "path": "./docs/md"
    }
  ]
}
